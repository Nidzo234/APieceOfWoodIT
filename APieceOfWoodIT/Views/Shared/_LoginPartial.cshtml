@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
        @Html.AntiForgeryToken()

<ul class="nav navbar-nav navbar-right">
    <li class="nav-item">
        <a class="btn btn-light mr-2" href="/manage">Поставки</a>
        
    </li>
    <li class="nav-item"><a class="btn btn-dark" href="javascript:document.getElementById('logoutForm').submit()">Log off</a></li>
</ul>
    }
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <a href="/Account/Login" class="btn btn-dark">Најави се</a>
    </ul>
}


@model IEnumerable<APieceOfWoodIT.Models.Order>
@{
    ViewBag.Title = "Index";
}

<h2>Сите нарачки</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.OrderId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedDate)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrderId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                @Html.ActionLink("Детали", "Details", "Orders", new { id = item.OrderId }, null)
            </td>
        </tr>
    }

</table>
